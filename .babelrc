{
  "plugins": [
    "transform-runtime",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "bber-cli": "./bber-cli",
        "bber-lib": "./bber-lib",
        "bber-modifiers": "./bber-modifiers",
        "bber-output": "./bber-output",
        "bber-plugins": "./bber-plugins",
        "bber-shapes": "./bber-shapes",
        "bber-templates": "./bber-templates",
        "bber-utils": "./bber-utils"
      }
    }]
  ],
  "presets": ["es2015", "stage-0"],
  "ignore": [
    "node_modules",
    "**/__tests__/**"
  ],
  "env": {}
}
