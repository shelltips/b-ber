{
    "name": "@canopycanopycanopy/b-ber-cli",
    "version": "0.1.6",
    "description": "",
    "main": "dist/index.js",
    "bin": {
        "bber": "dist/index.js"
    },
    "scripts": {
        "prepare.dist": "BABEL_ENV=production babel -d dist/ src/ --ignore **/__tests__/**",
        "clean": "rimraf dist",
        "prepare": "npm run clean && npm run prepare.dist",
        "build": "npm run prepare",
        "watch": "BABEL_ENV=production babel -d dist/ src/ --watch src",
        "start": "babel-node --trace-warnings ./src/index.js --presets es2015,stage-0",
        "test": "jest"
    },
    "author": "Triple Canopy <b-ber@canopycanopycanopy.com> (https://triplecanopy.github.io/)",
    "license": "GPL-3.0-or-later",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@canopycanopycanopy/b-ber-lib": "^0.1.6",
        "@canopycanopycanopy/b-ber-logger": "^0.1.6",
        "@canopycanopycanopy/b-ber-shapes": "^0.1.6",
        "@canopycanopycanopy/b-ber-tasks": "^0.1.6",
        "@canopycanopycanopy/b-ber-templates": "^0.1.6",
        "babel-runtime": "^6.26.0",
        "fs-extra": "^7.0.1",
        "lodash": "^4.17.11",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "jest": "^22.4.3",
        "rimraf": "^2.6.2"
    }
}
