{
  "name": "@canopycanopycanopy/b-ber-cli",
  "version": "0.1.0-alpha.16",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "bber": "dist/index.js"
  },
  "scripts": {
    "prepare.dist": "BABEL_ENV=production ../../node_modules/.bin/babel -d dist/ src/ --ignore **/__tests__/**",
    "clean": "rimraf dist",
    "prepare": "yarn clean && yarn prepare.dist",
    "build": "yarn prepare",
    "watch": "BABEL_ENV=production ../../node_modules/.bin/babel -d dist/ src/ --watch src",
    "start": "./node_modules/.bin/babel-node --trace-warnings ./src/index.js --presets es2015,stage-0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Maxwell Simmer <maxwell.simmer@gmail.com> (http://maxwellsimmer.com)",
  "license": "GNU GPLv3",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@canopycanopycanopy/b-ber-lib": "canary",
    "@canopycanopycanopy/b-ber-logger": "canary",
    "@canopycanopycanopy/b-ber-shapes": "canary",
    "@canopycanopycanopy/b-ber-tasks": "canary",
    "@canopycanopycanopy/b-ber-templates": "canary",
    "babel-runtime": "^6.26.0",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.5",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "rimraf": "^2.6.2"
  }
}
