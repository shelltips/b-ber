{
  "name": "bber",
  "version": "0.1.0",
  "description": "A command line epub maker.",
  "main": "./lib/cli.es6",
  "scripts": {
    "start": "babel-node ./lib/cli.es6 --presets es2015,stage-2",
    "lint": "./node_modules/.bin/eslint ./lib/cli.es6 lib/*.es6 lib/**/*.es6",
    "test": "ava --tap | tap-spec && npm run -s lint"
  },
  "bin": {
    "bber": "./lib/cli.es6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triplecanopy/b-ber-creator.git"
  },
  "author": "Maxwell Simmer",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "del": "^2.2.2",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-inject": "^4.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-sass": "^2.3.2",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "layouts": "^0.13.2",
    "lodash": "^4.16.1",
    "markdown-it": "^7.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-footnote": "^3.0.1",
    "mime-types": "^2.1.12",
    "mkdirp": "^0.5.1",
    "node-babel": "^0.1.2",
    "q": "^1.4.1",
    "recursive-readdir": "^2.1.0",
    "request": "^2.75.0",
    "require-dir": "^0.3.0",
    "unzip": "^0.1.11",
    "vinyl": "^1.2.0",
    "xmlbuilder": "^8.2.2",
    "yamljs": "^0.2.8",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "tap-spec": "^4.1.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.{js,jsx,es6}"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
