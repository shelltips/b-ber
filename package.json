{
  "name": "bber",
  "version": "0.1.0-alpha.1",
  "description": "A command line epub maker.",
  "main": "./src/cli.jsx",
  "scripts": {
    "__build": "./node_modules/.bin/babel -d lib/ src/",
    "build": "BABEL_ENV=production npm run __build",
    "bump": "npm version prerelease",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "istanbul": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec ./src/__tests__/ --compilers js:./node_modules/babel-register || true",
    "mocha": "./node_modules/.bin/mocha -R spec ./src/__tests__/ --compilers js:./node_modules/babel-register || true",
    "lint": "./node_modules/.bin/eslint ./src/cli.jsx src/*.jsx src/**/*.jsx || true",
    "prepublish": "BABEL_ENV=production npm run __build",
    "preversion": "npm test",
    "documentation": "./node_modules/.bin/jsdoc --verbose -c .jsdoc.json",
    "start": "./node_modules/.bin/babel-node ./src/cli.jsx --presets es2015,stage-0",
    "test": "npm run istanbul",
    "version": "BABEL_ENV=production npm run __build && git add -A"
  },
  "bin": {
    "bber": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/triplecanopy/b-ber-creator.git"
  },
  "author": "Maxwell Simmer",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "bber-editor": "git+https://github.com/triplecanopy/dillinger.git",
    "bunyan": "^1.8.5",
    "bunyan-format": "^0.2.1",
    "copy-dir": "^0.3.0",
    "css": "^2.2.1",
    "decompress": "^4.0.0",
    "epub-zipper": "^1.2.7",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "html-pdf": "^2.1.0",
    "htmlparser2": "^3.9.2",
    "image-size": "^0.5.0",
    "jshint": "^2.9.3",
    "layouts": "^0.13.2",
    "lodash": "^4.17.4",
    "markdown-it": "^7.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-front-matter": "^0.1.2",
    "mime-types": "^2.1.13",
    "mobi-zipper": "^0.1.5",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "opn": "^4.0.2",
    "postcss": "^5.2.6",
    "recursive-readdir": "^2.1.0",
    "serve-index": "^1.8.0",
    "vinyl": "^1.2.0",
    "xml2js": "^0.4.17",
    "yamljs": "^0.2.8",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.14.0",
    "babili": "^0.0.12",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.4.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "minami": "^1.1.1",
    "mocha": "^3.2.0",
    "tap-spec": "^4.1.1"
  },
  "engines": {
    "node": ">=6"
  }
}
